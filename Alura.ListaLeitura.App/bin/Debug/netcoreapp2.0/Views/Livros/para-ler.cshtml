<!DOCTYPE html>

<html lang="en" xmlns="http://www.w3.org/1999/xhtml">
<head>
    <meta charset="utf-8" />
    <title></title>
</head>
<body>
    <table>
        <tr>
            <th>Titulo</th>
            <th>Autor</th>
            <th>Ações</th>
        </tr>
        @foreach (var livro in ViewBag.Livros)
        {
            <tr>
                <td>@livro.Titulo</td>
                <td>@livro.Autor</td>
                <td></td>
            </tr>
        }
        </table>
</body>
</html>